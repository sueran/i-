}
page {
  background-color: #eee;
}
page .content {
  position: relative;
}
page .content .header {
  width: 100%;
  height: 200rpx;
  background-color: #187759;
  text-align: center;
  color: white;
  font-size: 30rpx;
  font-weight: 700;
  position: fixed;
}
page .content .wrap {
  position: absolute;
  top: 30rpx;
  left: 50%;
  -webkit-transform: translateX(-45vw);
          transform: translateX(-45vw);
  width: 90vw;
  margin: 0 auto;
  margin-bottom: 20rpx;
}
page .content .wrap_chart {
  border-radius: 10rpx;
  background-color: white;
  padding: 30rpx;
  position: relative;
}
page .content .wrap_access {
  box-sizing: border-box;
  border-radius: 10rpx;
  padding: 20rpx;
  width: 100%;
  background-color: white;
  margin-top: 20rpx;
}
page .content .wrap_access .line {
  border-bottom: 2rpx solid #eee;
  padding: 20rpx 0;
}
page .content .wrap_access .line_title {
  font-size: 35rpx;
  font-weight: 600;
  padding-bottom: 20rpx;
}
page .content .wrap_access .line_access {
  display: flex;
  justify-content: center;
}
page .content .wrap_access .line_access .accessOne {
  flex: 1;
}
page .content .wrap_access .line_access .accessOne text {
  color: red;
  margin-left: 20rpx;
}
page .content .wrap_access .line_access .accessTwo {
  flex: 1;
  border-left: 2rpx #ccc solid;
  padding-left: 20rpx;
}
page .content .wrap_access .line_access .accessTwo text {
  color: red;
  margin-left: 20rpx;
}

