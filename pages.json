{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "国家电网",
				"navigationBarBackgroundColor": "#187759"
			}
		},
		{
			"path": "pages/monitor/monitor",
